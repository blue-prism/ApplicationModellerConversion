<ConversionRules Name="IE to Chrome/Edge/Firefox Browser Conversion Rules" BPVersion="6.9.x" ObjectNameSuffix="">
  <AppTypeInfoConversionRules>
    <IDs>
      <ID>
        <fromID>BrowserAttach</fromID>
        <toID>BrowserAttach</toID>
      </ID>
      <ID>
        <fromID>BrowserLaunch</fromID>
        <toID>BrowserLaunch</toID>
      </ID>
    </IDs>
    <Parameters>
      <Parameter>
        <fromParameter>WindowTitle</fromParameter>
        <toParameter>WindowTitle</toParameter>
      </Parameter>
      <Parameter>
        <fromParameter>Path</fromParameter>
        <toParameter Prompt="true" PromptText="Please enter the full path to Chrome/Edge/Firefox browser executable (e.g. for Google Chrome, it can be &quot;C:\Program Files (x86)\Google\Chrome\Application\chrome.exe&quot;):" PValue="">Path</toParameter>
      </Parameter>
      <Parameter>
        <fromParameter>CommandLineParams</fromParameter>
        <toParameter>CommandLineParams</toParameter>
      </Parameter>
      <Parameter>
        <fromParameter>ProcessMode</fromParameter>
        <toParameter>ProcessMode</toParameter>
      </Parameter>
      <Parameter>
        <fromParameter>ExternalProcessTimeout</fromParameter>
        <toParameter>ExternalProcessTimeout</toParameter>
      </Parameter>
    </Parameters>
  </AppTypeInfoConversionRules>
  <Attributes>
    <Attribute>
      <fromAttribute/>
      <toAttribute datatype="text" AValue="">wCssSelector</toAttribute>
    </Attribute>
  </Attributes>
  <Elements>
    <Element>
      <fromElement>Application</fromElement>
      <toElement>Application</toElement>
      <Conditions />
      <Actions>
        <Action>
          <fromAction>DetachApplication</fromAction>
          <toAction>WebDetachApplication</toAction>
          <toActionArguments>
            <Argument>trackingid</Argument>
          </toActionArguments>
        </Action>
      </Actions>
      <ReadActions />
    </Element>
  </Elements>
</ConversionRules>